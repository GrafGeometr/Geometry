X = #(0.38684714+50.048355i)
Y = #(1.934236-37.669254i)
O_1 = project(#(63.00775+9.4777565i), midline(X, Y))
[red] c1 = circle(O_1, X)
O_2 = project(#(-43.665363+0.9671188i), midline(X, Y))
[red] c2 = circle(O_2, X)
A = choose(c1, time()/500)
[green] l = line(A, Y)
B = cintersect(Y, l, c2)
[violet] segment(O_1, X)
[violet] segment(O_2, X)
[violet] segment(O_1, A)
[violet] segment(O_2, B)
[violet] [fill] clockwise(X, O_1, A)
[violet] [fill] clockwise(X, O_2, B)
