[fill] c = circle(0, 60)
A = cproject(#(-56.092842+22.678915i), c)
B = cproject(#(57.992893+19.183134i), c)
[violet] l = segment(A, B)
M = midpoint(l)
P = cproject(#(-13.588008+54.352013i), c)
Q = cintersect(P, P, M, c)
R = cproject(#(37.959385+45.84139i), c)
S = cintersect(R, R, M, c)
[orange] [fill] polygon(P, Q, R, S)
X = intersect(P, S, l)
Y = intersect(R, Q, l)
[green] segment(X, Y)
