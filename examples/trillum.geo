A = #(-85.92846+89.16829i)
B = #(29.760408-1.3941998i)
C = #(-78.578316-24.564795i)
[gray] line(A, B)
[gray] line(A, C)
[orange] [fill] t = triangle(A, B, C)
l = bisector(C, A, B)
[red] c = circumcircle(t)
M = cintersect(A, l, c)
I = incenter(t)
[gray] incircle(t)
I_A = excenter(C, A, B)
[gray] excircle(C, A, B)
[blue] circle(M, I)
[violet] [equal1] segment(M, I)
[violet] [equal1] segment(M, B)
[violet] [equal1] segment(M, I_A)
[violet] [equal1] segment(M, C)
